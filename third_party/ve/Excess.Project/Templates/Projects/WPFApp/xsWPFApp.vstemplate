<VSTemplate Version="2.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
  <TemplateData>
    <Name>Excess WPF Application</Name>
    <Description>A project for creating a windows xs application</Description>
    <Icon>WPFApp.ico</Icon>
    <ProjectType>Excess</ProjectType>
    <SortOrder>40</SortOrder>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>WPFApplication</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <RequiredFrameworkVersion>4.0</RequiredFrameworkVersion>
  </TemplateData>
  <TemplateContent>
    <Project File="xsWPFApp.xsproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" OpenInEditor="true" TargetFileName="xsWindow.xaml">xsWindow.xaml</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="WPFApp.xml">WPFApp.xml</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="WPFApp.xs">WPFApp.xs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="WPFApp.project.xml">WPFApp.project.xml</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>
