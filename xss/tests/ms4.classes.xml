<xss:classes>
    <class id="fade_in" super="sequence" target_type="component">
        <at time="0">
            <animate property="alpha" from="0" to="70" in="1"/>
        </at>
        <at time="1">
            <animate property="alpha" from="70" to="100" in="1"/>
        </at>
    </class>
</xss:classes>
