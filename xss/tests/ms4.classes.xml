<xss:classes>
    <class id="fade_in" super="sequence">
        <at time="0">
            <animate property="target.alpha" from="0" to="0.3" in="1"/>
        </at>
        <at time="1">
            <animate property="target.alpha" from="0.3" to="1" in="1"/>
        </at>
    </class>
</xss:classes>
