<application src="ms3.app.xs" width="1000" height="1000">
    <resources>
        <image src="images/smile_grin_48.png"/>
        <image src="images/test2.png"/>
    </resources>

    <rectangle id="rr1" x="70" y="30" width="100" height="3" fill="black"/>
    <rectangle id="r1" x="120" y="20" width="3" height="0" fill="black"/>
    <rectangle id="r2" x="40" y="140" width="103" height="3" fill="black"/>
    <rectangle id="r3" x="20" y="20" width="3" height="103" fill="black"/>
    <circle id="c1" x="35" y="35" radius="60" stroke="black" line_width="20"/>
    <circle id="c2" x="75" y="350" radius="60" stroke="black" line_width="10"/>
    <img id="img1" x="100" y="100" width="130" height="130" src="images/test2.png"/>
    <img id="img2" x="300" y="300" width="80" height="80" src="images/test2.png"/>

    <sequence id="s1" auto_start="true">
        <at time="0">
            <animate property="c2.radius" from="0" to="50" in="20"/>
        </at>
        <at time="3">
            <animate property="r1.height" from="0" to="100" in="3"/>
        </at>
    </sequence>
</application>