<xss:code>
for(var i in application.children)
{
	for(var prop in i.properties)
	{
		out()
		{
            property id: <xss:e v="prop.id"/>
			property get: <xss:e v="compiler.property_get(prop, 'this')"/>
			property set: <xss:e v="compiler.property_set(prop, 'that', '20')"/>
		}
	}

	for(var mthd in i.methods)
	{
		out()
		{
            method id: <xss:e v="mthd.id"/>
			code: 
			<xss:e v="mthd.code.render()"/>
		}
	}
}
</xss:code>
