<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="XKPLayout">
		<attr name="layout_x" format="dimension"/>
		<attr name="layout_y" format="dimension"/>
		<attr name="autosize_x" format="boolean"/>
		<attr name="autosize_y" format="boolean"/>
		<attr name="placement">
			<enum name="none"   value="0"/>
			<enum name="top"    value="1"/>
			<enum name="left"   value="2"/>
			<enum name="right"  value="3"/>
			<enum name="bottom" value="4"/>
			<enum name="client" value="5"/>
			<enum name="center" value="6"/>
		</attr>
	</declare-styleable>
</resources>
