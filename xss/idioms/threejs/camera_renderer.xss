<xss:parameter id="obj"/>
<xss:parameter id="index"/>	

<xss:code>

	if(obj.id == "")
		obj.id = "camera_w_id_"+index;
		
	if(obj.class_name == "quake_camera")
		compiler.xss("param_in_obj.xss", obj);
	else
	{
		out(){var <xss:e v="obj.id"/> = }compiler.xss("../common-js/instantiator.xss", obj);	
	}
	out(marker = "camera_on_render", marker_source = "entry")
		{
		set_active_camera(<xss:e v="obj.id"/>);
		}	
</xss:code>
