				<parameter id="radius" property="radius" runtime="true"/>
				<parameter id="detail" property="detail" runtime="true"/>				
			</constructor_params>
			<property id="radius" type="float" dont_init="true"/>
			<property id="detail" type="float" dont_init="true"/>			
		</class>
		
