<module id="threejs" src="module.xs">

	<types>
		
		<class id="parameter_braces"/>
		
		<class id="composite_material" output_id="Array"/>
		
		<class id="basic_material" super="parameter_braces" output_id="THREE.MeshBasicMaterial">
			<array id="constructor_params">
				<parameter property="color"/> 
				<parameter property="map"/>
				<parameter property="wireframe"/>
				<parameter property="wireframe_linewidth"/>
				<parameter property="opacity"/>
				<parameter property="texture_src"/>				
				<parameter property="shading"/>
				<parameter property="vertex_colors"/>
				<parameter property="env_map"/>
				<parameter property="combine"/>
				<parameter property="reflectivity"/>
				<parameter property="shininess"/>
				<parameter property="specular"/>
				<parameter property="transparent"/>
			</array>
			<property id="color" type="string" dont_init="true"/> 
			<property id="map" type="string" dont_init="true"/>
			<property id="wireframe" type="string" dont_init="true"/>
			<property id="wireframe_linewidth" type="string" output_id="wireframeLinewidth" dont_init="true"/>
			<property id="opacity" type="string" dont_init="true"/>
			<property id="texture_src" type="string" dont_init="true"/>				
			<property id="shading" type="string" dont_init="true"/>
			<property id="vertex_colors" type="string" output_id="vertexColors" dont_init="true"/>
			<property id="env_map" type="string" output_id="envMap" dont_init="true"/>
			<property id="combine" type="string" dont_init="true"/>
			<property id="reflectivity" type="string" dont_init="true"/>
			<property id="shininess" type="string" dont_init="true"/>
			<property id="specular" type="string" dont_init="true"/>
			<property id="transparent" type="string" dont_init="true"/>
		</class>
		
		<class id="lambert_material" super="parameter_braces" output_id="THREE.MeshLambertMaterial">
			<array id="constructor_params">
				<parameter property="color"/> 
				<parameter property="map"/>
				<parameter property="wireframe"/>
				<parameter property="wireframe_linewidth"/>
				<parameter property="opacity"/>
				<parameter property="texture_src"/>				
				<parameter property="shading"/>
				<parameter property="vertex_colors"/>
				<parameter property="env_map"/>
				<parameter property="combine"/>
				<parameter property="reflectivity"/>
				<parameter property="shininess"/>
				<parameter property="specular"/>
				<parameter property="transparent"/>
			</array>
			<property id="color" type="string" dont_init="true"/> 
			<property id="map" type="string" dont_init="true"/>
			<property id="wireframe" type="string" dont_init="true"/>
			<property id="wireframe_linewidth" type="string" output_id="wireframeLinewidth" dont_init="true"/>
			<property id="opacity" type="string" dont_init="true"/>
			<property id="texture_src" type="string" dont_init="true"/>				
			<property id="shading" type="string" dont_init="true"/>
			<property id="vertex_colors" type="string" output_id="vertexColors" dont_init="true"/>
			<property id="env_map" type="string" output_id="envMap" dont_init="true"/>
			<property id="combine" type="string" dont_init="true"/>
			<property id="reflectivity" type="string" dont_init="true"/>
			<property id="shininess" type="string" dont_init="true"/>
			<property id="specular" type="string" dont_init="true"/>
			<property id="transparent" type="string" dont_init="true"/>
		</class>

		<class id="phong_material" super="parameter_braces" output_id="THREE.MeshPhongMaterial">
			<array id="constructor_params">
				<parameter property="color"/> 
				<parameter property="map"/>
				<parameter property="wireframe"/>
				<parameter property="wireframe_linewidth"/>
				<parameter property="opacity"/>
				<parameter property="texture_src"/>				
				<parameter property="shading"/>
				<parameter property="vertex_colors"/>
				<parameter property="env_map"/>
				<parameter property="combine"/>
				<parameter property="reflectivity"/>
				<parameter property="shininess"/>
				<parameter property="specular"/>
				<parameter property="transparent"/>
			</array>
			<property id="color" type="string" dont_init="true"/> 
			<property id="map" type="string" dont_init="true"/>
			<property id="wireframe" type="string" dont_init="true"/>
			<property id="wireframe_linewidth" type="string" output_id="wireframeLinewidth" dont_init="true"/>
			<property id="opacity" type="string" dont_init="true"/>
			<property id="texture_src" type="string" dont_init="true"/>				
			<property id="shading" type="string" dont_init="true"/>
			<property id="vertex_colors" type="string" output_id="vertexColors" dont_init="true"/>
			<property id="env_map" type="string" output_id="envMap" dont_init="true"/>
			<property id="combine" type="string" dont_init="true"/>
			<property id="reflectivity" type="string" dont_init="true"/>
			<property id="shininess" type="string" dont_init="true"/>
			<property id="specular" type="string" dont_init="true"/>
			<property id="transparent" type="string" dont_init="true"/>
		</class>

		<class id="normal_material" output_id="THREE.MeshNormalMaterial"/>

		<class id="face_material" output_id="THREE.MeshFaceMaterial"/>
		
		<class id="sound" output_id="THREE.Sound">
			<array id="constructor_params">
				<parameter property="sources"/> 
				<parameter property="radius"/>
				<parameter property="volume"/>
				<parameter property="loop"/>				
			</array>
			<property id="sources" type="string" dont_init="true"/>
			<property id="radius" type="string" dont_init="true"/>
			<property id="volume" type="string" dont_init="true"/>
			<property id="loop" type="string" dont_init="true"/>
		</class>
		
		<class id="texture" output_id="THREE.Texture">
			<array id="constructor_params">
				<parameter property="image"/> 								
			</array>
			<property id="image" type="string" dont_init="true"/>
		</class>

        <class id="vector3d">
            <property id="x" type="float" dont_init="true"/>
            <property id="y" type="float" dont_init="true"/>
            <property id="z" type="float" dont_init="true"/>
        </class>

        <class id="meshable">						
            <property id="color" type="string" set_fn="materials[ 0 ].color.setHex"/>
			<property id="overdraw" type="string"/>
			<property id="double_sided" type="string" output_id="doubleSided"/>
			<property id="materials" type="string" dont_init="true"/>

            <property id="position" type="vector3d" dont_init="true"/>
        </class>
	
		<class id="transform" output_id="THREE.Object3D"/>
			
		<class id="transformation">		
            <property id="x" type="string" dont_init="true"/>
			<property id="y" type="string" dont_init="true"/>
			<property id="z" type="string" dont_init="true"/>		
		</class>
	
		<class id="scale" super="transformation"/>
 		<class id="rotation" super="transformation"/>	 
		<class id="position" super="transformation"/>
			
			
		<class id="light"/>    
		
		<class id="ambient_light" super="light" output_id="THREE.AmbientLight"/>       
	
		<class id="directional_light" super="light" output_id="THREE.DirectionalLight">
			<array id="constructor_params">
				<parameter property="hex"/>
				<parameter property="intensity"/>
				<parameter property="distance"/>
				<parameter property="cast_shadow" output_id="castShadow"/>  				
			</array>
				<property id="hex" type="string" dont_init="true"/>
				<property id="intensity" type="string" dont_init="true"/>
				<property id="distance" type="string" dont_init="true"/>
				<property id="cast_shadow" output_id="castShadow" type="string" dont_init="true"/>
		</class>
	
		<class id="point_light" super="light" output_id="THREE.PointLight">
			<array id="constructor_params">
				<parameter property="hex"/>
				<parameter property="intensity"/>
				<parameter property="distance"/>           		
			</array>
				<property id="hex" type="string" dont_init="true"/>
				<property id="intensity" type="string" dont_init="true"/>
				<property id="distance" type="string" dont_init="true"/>
		</class>
	
		<class id="camera" output_id="THREE.Camera">
			<array id="constructor_params">				
				<parameter property="fov"/>
				<parameter property="aspect"/>
				<parameter property="near"/>
				<parameter property="far"/>     
				<parameter property="target"/>  			
			</array>
			<property id="hex" type="string" /> 
			<property id="fov" type="string" dont_init="true"/>
			<property id="aspect" type="string" dont_init="true"/>
			<property id="near" type="string" dont_init="true"/>
			<property id="far" type="string" dont_init="true"/>
			<property id="target" type="string" dont_init="true"/>				
		</class>
	
		<class id="quake_camera" super="parameter_braces" output_id="THREE.QuakeCamera">
			<array id="constructor_params"> 
				<parameter property="hex"/>
				<parameter property="fov"/>
				<parameter property="aspect"/>
				<parameter property="near"/>
				<parameter property="far"/>     
				<parameter property="target"/> 
				<parameter property="movement_speed" output_id="movementSpeed"/>
				<parameter property="look_speed" output_id="lookSpeed"/>
				<parameter property="no_fly" output_id="noFly"/>
				<parameter property="look_vertical" output_id="lookVertical"/>
				<parameter property="auto_forward" output_id="autoForward"/>			
			</array>
			<property id="hex" type="string" dont_init="true"/> 
			<property id="fov" type="string" dont_init="true"/>
			<property id="aspect" type="string" dont_init="true"/>
			<property id="near" type="string" dont_init="true"/>
			<property id="far" type="string" dont_init="true"/>
			<property id="target" type="string" dont_init="true"/>
			<property id="movement_speed" output_id="movementSpeed" type="string" dont_init="true"/> 
			<property id="look_speed" output_id="lookSpeed" type="string" dont_init="true"/>
			<property id="no_fly" output_id="noFly" type="string" dont_init="true"/>
			<property id="look_vertical" output_id="lookVertical" type="string" dont_init="true"/>
			<property id="auto_forward" output_id="autoForward" type="string" dont_init="true"/>
		</class>
	
		<class id="plane" super="meshable" output_id="THREE.Plane">
			<array id="constructor_params">
				<parameter property="width"/>
				<parameter property="height"/>
				<parameter property="segments_width" output_id="segmentsWidth"/>
				<parameter property="segments_height" output_id="segmentsHeight"/>
			</array>
			<property id="width" type="string" dont_init="true"/>
			<property id="height" type="string" dont_init="true"/>
			<property id="segments_width" output_id="segmentsWidth" type="string" dont_init="true"/>
			<property id="segments_height" output_id="segmentsHeight" type="string" dont_init="true"/>
		</class>
	
		<class id="cube" super="meshable" output_id="THREE.Cube">
			<array id="constructor_params">
				<parameter property="width"/>
				<parameter property="height"/>
				<parameter property="depth"/>      
				<parameter property="segments_width" output_id="segmentsWidth"/>
				<parameter property="segments_height" output_id="segmentsHeight"/>
				<parameter property="segments_depth" output_id="segmentsDepth"/>
				<parameter property="cube_materials"/>
				<parameter property="flipped"/>
				<parameter property="sides"/>			
			</array>
			<property id="width" type="string" dont_init="true"/> 
			<property id="height" type="string" dont_init="true"/>
			<property id="depth" type="string" dont_init="true"/>
			<property id="segments_width" output_id="segmentsWidth" type="string" dont_init="true"/>
			<property id="segments_height" output_id="segmentsHeight" type="string" dont_init="true"/>
			<property id="segments_depth" output_id="segmentsDepth" type="string" dont_init="true"/>				
			<property id="cube_materials" type="string" dont_init="true"/>
			<property id="flipped" type="string" dont_init="true"/>
			<property id="sides" type="string" dont_init="true"/>
		</class>
	
		<class id="cylinder" super="meshable" output_id="THREE.Cylinder">
			<array id="constructor_params">
				<parameter property="numsegs"/>
				<parameter property="toprad"/>
				<parameter property="botrad"/>      
				<parameter property="height"/>
				<parameter property="topoffset"/>
				<parameter property="bottomoffset"/>			
			</array>	
			<property id="numsegs" type="string" dont_init="true"/> 
			<property id="toprad" type="string" dont_init="true"/>
			<property id="botrad" type="string" dont_init="true"/>
			<property id="height" type="string" dont_init="true"/>
			<property id="topoffset" type="string" dont_init="true"/>
			<property id="bottomoffset" type="string" dont_init="true"/>	
		</class>
	
		<class id="sphere" super="meshable" output_id="THREE.Sphere">
			<array id="constructor_params">
				<parameter property="radius"/>
				<parameter property="segments_width" output_id="segmentsWidth"/>
				<parameter property="segments_height" output_id="segmentsHeight"/>			
			</array>
			<property id="radius" type="string" dont_init="true"/>
			<property id="segments_width" output_id="segmentsWidth" type="string" dont_init="true"/>
			<property id="segments_height" output_id="segmentsHeight" type="string" dont_init="true"/>
		</class>
	
		<class id="icosahedron" super="meshable" output_id="THREE.Icosahedron">
			<array id="constructor_params">
				<parameter property="subdivisions"/>			
			</array>
			<property id="subdivisions" type="string" dont_init="true"/>
		</class>
	
		<class id="particle" super="meshable" output_id="THREE.Particle"/>
    
		<class id="scene" output_id="THREE.Scene"/>	
	
		<class id="texture_cube" output_id="THREE.ImageUtils.loadTextureCube">
			<array id="constructor_params">
				<parameter property="array"/>
			</array>
			<property id="array" type="string" dont_init="true"/>
		</class>
	
		<class id="bin_loader" output_id="THREE.BinaryLoader">
			<array id="constructor_params">
				<parameter property="model"/>
				<parameter property="callback"/>			
			</array>
			<property id="model" type="string" dont_init="true"/>
			<property id="callback" type="string" dont_init="true"/>
		</class>		
		
	</types>
	
</module>
