<xss:parameter id="i"/>
<xss:parameter id="index"/>

<xss:code>
		if(i.instantiator)
			compiler.xss(i.instantiator, i, index);
		else{						
			out(){var <xss:e v="i.id"/> = }
			compiler.xss("../common-js/instantiator.xss", i);						
			}	
		if(i.parent.class_name == "transform"){			
				out(){
				<xss:e v="i.parent.id"/>.addChild(<xss:e v="i.id"/>);
				scene.addObject(<xss:e v="i.parent.id"/>);
				}
			}				
</xss:code>
