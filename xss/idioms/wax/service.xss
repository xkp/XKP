<xss:parameter id="it"/>

<xss:code>
    
compiler.log("HERE");

var service_name = it.id;    
var mthd = application.get_method(service_name);

if (!mthd)
    compiler.error("Service implementor not found", name = service_name);

out()
{
    <xss:e v="mthd.code.render()"/>
    request.end();
    return;    
}
</xss:code>
