<xss:parameter id="res"/>

<xss:code>		
	out()
	{
		for(var i = 0; i < def_package_items.length; i++)
        {						
			if(def_package_items[i].id == "<xss:e v="res.parent.id"/>")
				def_package_items[i].animations.push
				({
					id:			"<xss:e v="res.id"/>",
					frame_row:	<xss:e v="res.frame_row"/>,
					frame_col:	<xss:e v="res.frame_col"/>,
					frames:		<xss:e v="res.frames"/>,
					step:		<xss:e v="res.step"/>
				});
		}		
	}
</xss:code>
