<xss:parameter id="res"/>

<xss:code>
		if(res.resource_type == "RESOURCE_SOUND")
			out(marker = "audio_resources", marker_source = "entry")
			{
			<source src = streamer.get_resource('<xss:e v="res.src"/>').asset/>			
			}
			
		out()
		{
		id:"<xss:e v="res.id"/>",
		resource_type:<xss:e v="res.resource_type"/>,
		src:"<xss:e v="res.src"/>"
		}		
</xss:code>
