<idiom id="dotnet_test" src="ws-node.xs">
    <enum id="test_enum">
        <item id="te1"/>
        <item id="te2"/>
        <item id="te3"/>
    </enum>

    <class id="Application">
        <accept_child type="*" append="as_property"/>
    </class>
    
    <class id="simple_class" output_id="SimpleClass">
        <constructor>
            <argument id="name" type="string"/>
        </constructor>
        
        <method id="simple_method" return_type="string">
            <argument id="what" type="var"/>
            <argument id="e" type="test_enum"/>
        </method>

        <property id="simple_property" type="int">
            <get text="my_stuff.simple_property"/>
            <set global="true" text="my_stuff.set_simple_property({path}, {value})"/>
        </property>

        <event id="simple_event">
            <argument id="x" type="float"/>
            <argument id="y" type="float"/>
        </event>
    </class>

    <class id="user_class" output_id="UserClass">
        <accept_child type="simple_class" append="as_property"/>
        <accept_parent type="*" append="as_property"/>
        <reject_child type="*"/>

        <constructor>
        </constructor>

        <constructor>
            <argument id="id" type="string" />
            <argument id="params" type="array" array_type="string"/>
            <argument id="owner" type="simple_class" nullable="true"/>
            <argument id="name" type="string" default_value="Joe"/>
        </constructor>

        <method id="simple_method" return_type="string">
            <argument id="what" type="var"/>
            <argument id="e" type="test_enum"/>
        </method>

        <property id="simple_property" type="int">
            <get text="my_stuff.simple_property"/>
            <set global="true" text="my_stuff.set_simple_property({path}, {value})"/>
        </property>

        <property id="array_property" type="array" array_type="string">
        </property>

        <property id="object_property">
        </property>

        <simple_class id="child_object">
        </simple_class>

        <event id="simple_event">
            <argument id="x" type="float"/>
            <argument id="y" type="float"/>
        </event>
    </class>
</idiom>