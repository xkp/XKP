namespace <xss:e v="application.name"/>
{
    partial class <xss:e v="application.name"/>_Form
    {
        <xss:code>
        var app = application.root;
        var app_name = application.name + "_Form";
        var app_instances = winforms.instances;

        compiler.xss("Designer.xss", app, instances = app_instances, name = app_name);
        </xss:code>
    }
}

