<xss:parameter id="it"/>		ArrayList tabs = new ArrayList(Arrays.asList( new String [] {<xss:code>	var tabs_prop = it.get_property("tabs");	int sz_tabs = 0;	if(tabs_prop.type.is_array)		sz_tabs = tabs_prop.value.size;		bool first = true;	for(var tab in tabs_prop.value)	{		if(first)		{			first = false;			out()			{				"<xss:e value="tab.value"/>"			}		}		else		{			out()			{				, "<xss:e value="tab.value"/>"			}		}	}</xss:code>			} ));        util.setupTabHost(R.id.<xss:e value="it.output_id"/>, tabs);