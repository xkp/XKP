<module id="sequence" src="sequence.xs">
    <types>
        <class id="sequence" event_scope="application">
            <property id="loop" type="bool">
                <get text="getLoop()"/>
                <set text="setLoop({value})"/>
            </property>

            <property id="target" type="var"/>
            <method id="stop"/>
        </class>
    </types>
</module>